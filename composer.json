{
  "name": "oliverde8/php-etl-bundle",
  "type": "library",
  "description": "Allow usage of the PHP-ETL library in symfony framework.",
  "license": "MIT",
  "keywords": ["symfony-ux"],
  "authors": [
    {
      "name": "Oliver de Cramer",
      "homepage": "http://oliver-decramer.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "oliverde8/php-etl": "^v1.2.0-alpha1|^v2.0.0-alpha1",
    "symfony/framework-bundle": "^7.0|^8.0",
    "symfony/messenger": "^7.0|^8.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Oliverde8\\PhpEtlBundle\\": "."
    }
  },
  "require-dev": {
    "symfony/monolog-bundle": "^3.7",
    "symfony/console": "^7.0|^8.0"
  }
}
