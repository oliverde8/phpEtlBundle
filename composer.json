  "authors": [
    {
      "name": "Oliver de Cramer",
      "homepage": "http://oliver-decramer.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "oliverde8/php-etl": "^v1.2.0-alpha1",
    "symfony/framework-bundle": "^5.4|^6.0|^7.0",
    "symfony/messenger": "^5.4|^6.0|^7.0",
